{
  "name": "wing-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "export": "next build && next export",
    "build:mobile": "next build && mkdir -p out && cp -r .next/static out/ && cp -r public/* out/ 2>/dev/null || true",
    "sync:ios": "npx cap sync ios",
    "sync:android": "npx cap sync android",
    "open:ios": "npx cap open ios",
    "open:android": "npx cap open android",
    "run:ios": "npm run build:mobile && npm run sync:ios && npm run open:ios",
    "run:android": "npm run build:mobile && npm run sync:android && npm run open:android",
    "mobile:dev:ios": "CAP_SERVER_URL=http://127.0.0.1:3000 npm run sync:ios && npm run open:ios",
    "mobile:dev:android": "CAP_SERVER_URL=http://10.235.240.85:3000 npm run sync:android && npm run open:android",
    "mobile:prod": "CAP_SERVER_URL=https://mysteryverse.co.in npm run sync:ios && npm run sync:android",
    "type-check": "tsc --noEmit",
    "db:generate": "npx prisma migrate dev",
    "db:migrate": "npx prisma migrate deploy",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "postinstall": "npx prisma generate"
  },
